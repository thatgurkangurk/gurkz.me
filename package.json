		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"db:push": "tsx --no-warnings node_modules/drizzle-kit/bin.cjs --config ./drizzle.config.ts push",
		"db:generate": "tsx --no-warnings node_modules/drizzle-kit/bin.cjs --config ./drizzle.config.ts generate",
		"db:migrate": "tsx --no-warnings node_modules/drizzle-kit/bin.cjs --config ./drizzle.config.ts migrate",
		"db:studio": "tsx --no-warnings node_modules/drizzle-kit/bin.cjs --config ./drizzle.config.ts studio"
	},
